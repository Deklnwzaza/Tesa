<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../head %>
  <style>
    table, th, td {
      border: 1px solid black;
      border-collapse: collapse;
    }
    th, td {
      padding: 5px;
      text-align: left;
    }
  </style>
</head>
<body class="container">
<header>
    <% include ../header %>
</header>
<main>
  <div class="container">
    <% if(data.length != 0) {%>
    <% for(var i = 0; i < data.length; ++i) { %>
    <table style="width:100%">
      <tr>
        <th>No.</th>
        <th>teamID</th>
        <th>description</th>
        <th>date</th>
      </tr>
        <% for(var i = 0; i < data.length; ++i) {%>
      <tr>
        <td><%= i + 1 %></td>
        <td><%= data[i].teamID %></td>
        <td><%= data[i].description %></td>
        <td><%= data[i].date %></td>
      </tr>
        <% } %>

    </table>
    <% } %>
    <%}%>
  </div>
</main>
<footer>
    <% include ../footer %>
</footer>

</body>
</html>